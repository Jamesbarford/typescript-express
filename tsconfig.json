{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2017", "es7", "es6"],
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true
  },
  "exclude": ["./node_modules", "dist"]
}
